---
interface Props {
	width: string;
	height: string;
}

const { width, height } = Astro.props;
---

<picture style={{ width: `calc(${width} * 2)` }} class="github-profile">
	<img style={{ height: height, width: width }} src="https://avatars.githubusercontent.com/u/134110830?v=4" alt="Github Profile Picture" />
</picture>

<style>
	.github-profile {
        display: grid;
        place-items: center;

	}
</style>
